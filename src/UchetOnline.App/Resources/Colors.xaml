<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="PrimaryColor">#FF2F88FF</Color>
    <Color x:Key="SecondaryColor">#FFEDF3FF</Color>
    <Color x:Key="AccentColor">#FF3BD6B0</Color>
    <Color x:Key="TextColor">#FF1F1F1F</Color>
    <Color x:Key="MutedTextColor">#FF6B7280</Color>
    <SolidColorBrush x:Key="PrimaryBrush" Color="{StaticResource PrimaryColor}" />
    <SolidColorBrush x:Key="SecondaryBrush" Color="{StaticResource SecondaryColor}" />
    <SolidColorBrush x:Key="AccentBrush" Color="{StaticResource AccentColor}" />
    <SolidColorBrush x:Key="TextBrush" Color="{StaticResource TextColor}" />
    <SolidColorBrush x:Key="MutedTextBrush" Color="{StaticResource MutedTextColor}" />
    <SolidColorBrush x:Key="CardBackgroundBrush" Color="#FFFFFFFF" />
</ResourceDictionary>
